{
    "predictor_name": "GenericTimeSeriesPredictor",
    "predictor_config": {
        "model_adapter_class": "MitraModelAdapter",
        "model_adapter_config": {
            "model_config": {
                "device": "cuda",
                "fine_tune": false,
                "n_estimators": 4
            }
        },
        "worker_config": {
            "num_workers_per_gpu": 1
        }
    },
    "features": {
        "RunningIndexFeature": {},
        "AdditionalCalendarFeature": {
            "additional_seasonal_features": {
                "second_of_minute": [60],
                "minute_of_hour": [60]
            }
        },
        "AutoSeasonalFeature": {
            "config": {
                "max_top_k": 5,
                "detrend_type": "linear",
                "zero_padding_factor": 2,
                "exclude_zero": true
            }
        }
    },
    "context_length": 2048
}