{
    "predictor_name": "GenericTimeSeriesPredictor",
    "predictor_config": {
        "model_adapter_class": "LinearRegressionModelAdapter",
        "model_adapter_config": {
            "model_config": {
                "fit_intercept": true,
                "copy_X": true,
                "n_jobs": -1,
                "positive": false
            },
            "inference_config": {
                "fit": {},
                "predict": {}
            }
        },
        "worker_class": "CPUParallelWorker",
        "worker_config": {
            "num_workers": 32
        }
    },
    "features": {
        "RunningIndexFeature": {},
        "AdditionalCalendarFeature": {
            "additional_seasonal_features": {
                "second_of_minute": [60],
                "minute_of_hour": [60]
            }
        },
        "AutoSeasonalFeature": {
            "config": {
                "max_top_k": 5,
                "detrend_type": "linear",
                "zero_padding_factor": 2,
                "exclude_zero": true
            }
        }
    },
    "context_length": 4096
} 